  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Test part 1",
      "type": "python",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${relativeFileDirname}/.."
      },
      "program": "${file}",
      "args": [
        "${relativeFileDirname}/test.txt",
        "1"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Guess part 1",
      "type": "python",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${relativeFileDirname}/.."
      },
      "program": "${file}",
      "args": [
        "${relativeFileDirname}/input.txt",
        "1"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Test part 2",
      "type": "python",
      "request": "launch",
      "env": {
        "PYTHONPATH": "${relativeFileDirname}/.."
      },
      "program": "${file}",
